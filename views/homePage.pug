extends header

block content
    div.accountbg
    div.wrapper-page
        div.display-table
            div.display-table-cell
                div.container
                    div.row
                        div.col-md-6
                            img.img-fluid(src='assets/images/extra.png' alt='')
                        div#home-form.col-md-6
                            div.card
                                div.card-body
                                    div.text-center.pt-3
                                        a(href='index.html')
                                            img(src='assets/images/logo-h-lg.png' alt='logo' height='22')
                                    div.px-3.pb-3
                                        form.form-horizontal.m-t-20.mb-0(action='http://mannatthemes.com/urora/horizontal/index.html')
                                            div.form-group.row
                                                div.col-12
                                                    input.form-control(type='text' required='' placeholder='Username')
                                            div.form-group.row
                                                div.col-12
                                                    input.form-control(type='password' required='' placeholder='Password')
                                            div.form-group.row
                                                div.col-12
                                                    div.custom-control.custom-checkbox
                                                        input#customCheck1.custom-control-input(type='checkbox')
                                                        label.custom-control-label(for='customCheck1') Remember me
                                            div.form-group.text-right.row.m-t-20
                                                div.col-12
                                                    button.btn.btn-primary.btn-raised.btn-block.waves-effect.waves-light(type='submit') Log In
                                            div.form-group.m-t-10.mb-0.row
                                                div.col-sm-7.m-t-20
                                                    a.text-muted(href='#')
                                                        i.mdi.mdi-lock
                                                        |  Forgot your password ?
                                                div.col-sm-5.m-t-20
                                                    a.text-muted(href='/123')
                                                        i.mdi.mdi-account-circle
                                                        |  Create an account ?
    // jQuery
    script(src='javascript/homeForm.js')

    script(src='assets/js/jquery.min.js')
    script(src='assets/js/popper.min.js')
    script(src='assets/js/bootstrap-material-design.js')
    script(src='assets/js/modernizr.min.js')
    script(src='assets/js/detect.js')
    script(src='assets/js/fastclick.js')
    script(src='assets/js/jquery.slimscroll.js')
    script(src='assets/js/jquery.blockUI.js')
    script(src='assets/js/waves.js')
    script(src='assets/js/jquery.nicescroll.js')
    script(src='assets/js/jquery.scrollTo.min.js')

    // App js
    script(src='assets/js/app.js')








